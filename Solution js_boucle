ex1
function sumNumber(num) { 
    let s=0
    for(let i=0;i<=num;i++){
     s=s+i
    }
    
    return s
    }
    console.log(sumNumber(5))


ex2
  function reverse(array) { 
     let arrReversed=array.reverse()
     
     return arrReversed }
     
     console.log(reverse([1, 2, 3, 4]))

ex3
function getLastItem(array) { 
     
     return array[array.length-1]}
     console.log(getLastItem([1, 2, 3, "a"]))
ex4
 function concat(array1,  array2) {
    return array1.concat(array2)}
  console.log(concat([0, 10, 6],[900, 5, 60]))
ex5
function wordLengths(array) {
       let arr2=[] 
        for(let i=0;i<array.length;i++){
       arr2= arr2.concat(array[i].length)
    }
    
       return arr2  }
       console.log(wordLengths(["ahmed","aaaa"]))
ex6
function sum(array) { 
          let n=0
        for(let i=0;i<array.length;i++){
       n= n+array[i]
    }
    
       return n  }
         
         console.log(sum([4, 10, 6]))

ex7
  function noOdds(array) { 
  return array.filter(element => element % 2==0); 
  }
console.log(noOdds([1, 2, 3, 4, 5, 6, 7, 8]))

